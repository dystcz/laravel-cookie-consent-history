{
    "name": "dystcz/laravel-cookie-consent-history",
    "description": "This package allows you to simply store cookie consent history for users, so you can access it later in the unfortunate event of inspection or something.",
    "keywords": [
        "dystcz",
        "laravel-cookie-consent-history"
    ],
    "homepage": "https://github.com/dystcz/laravel-cookie-consent-history",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Theimer",
            "email": "jakub@dy.st",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dystcz\\LaravelCookieConsentHistory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dystcz\\LaravelCookieConsentHistory\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dystcz\\LaravelCookieConsentHistory\\LaravelCookieConsentHistoryServiceProvider"
            ],
            "aliases": {
                "LaravelCookieConsentHistory": "Dystcz\\LaravelCookieConsentHistory\\LaravelCookieConsentHistoryFacade"
            }
        }
    }
}
